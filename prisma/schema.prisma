  Religion                 Religion?                  @relation(fields: [religionId], references: [id])
  EmployeePermanentAddress EmployeePermanentAddress[] @relation("EmployeePermanentAddressToStudentInfo")
  EmployeePresentAddress   EmployeePresentAddress[]   @relation("EmployeePresentAddressToStudentInfo")
