model Test {
  id          Int     @id @default(autoincrement())
  name        String?
  descriptoin String?
}

model Thana {
  id                       Int                        @id @default(autoincrement())
  thanaName                String
  orgId                    Int?
  serialNo                 Int?
  activeStatus             Boolean                    @default(true)
  createdDate              String?
  createdTime              String?
  createdBy                Int?
  createdAt                DateTime                   @default(now())
  updatedDate              String?
  updatedTime              String?
  updatedAt                DateTime?
  updatedBy                Int?
  thanaDes                 String?
  dristrictId              Int?
