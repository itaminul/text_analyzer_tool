  id           Int       @id @default(autoincrement())
  username     String    @unique
  password     String
  mobileNumber String?
  emailAddress String?
  deptId       Int?
  desigId      Int?
  roleId       Int?
  orgId        Int?
  companyId    Int?
  activeStatus Boolean   @default(true)
